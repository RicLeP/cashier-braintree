{
    "name": "moonbaselabs/cashier-braintree",
    "description": "Laravel Cashier provides an expressive, fluent interface to Braintree's subscription billing services.",
    "keywords": ["laravel", "braintree", "billing", "cashier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "braintree/braintree_php": "~3.38",
        "dompdf/dompdf": "^1.0.2",
        "illuminate/contracts": "^5.7|^6.0|^7.0|^8.0",
        "illuminate/database": "^5.7|^6.0|^7.0|^8.0",
        "illuminate/http": "^5.7|^6.0|^7.0|^8.0",
        "illuminate/routing": "^5.7|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.7|^6.0|^7.0|^8.0",
        "illuminate/view": "^5.7|^6.0|^7.0|^8.0",
        "nesbot/carbon": "^1.26.3|^2.0",
        "symfony/http-kernel": "~5.0"
    },
    "require-dev": {
        "ext-json": "*",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0"
        },
        "laravel": {
            "providers": [
                "Laravel\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
